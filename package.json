{
  "name": "cor-matrix",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "license": "MIT",
  "author": "Presidio",
  "scripts": {
    "db:push": "bunx drizzle-kit push",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:studio": "bunx drizzle-kit studio",
    "dev:api": "bun run --watch src/api/index.ts",
    "api": "bun run src/api/index.ts",
    "build:client": "bun run src/client/build.ts",
    "build:cli": "bun run src/cli/build.ts",
    "build:api": "docker build -t cor-matrix-api -f src/api/Dockerfile .",
    "format": "prettier --write .",
    "test:integration": "vitest run tests/integration/",
    "test:integration:watch": "vitest tests/integration/"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/jwt": "^1.3.1",
    "@elysiajs/swagger": "^1.3.0",
    "@libsql/client": "^0.15.8",
    "dotenv": "^16.5.0",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.0",
    "elysia": "^1.3.3",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "ulid": "^3.0.0",
    "yargs": "^18.0.0",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/supertest": "^6.0.2",
    "@types/yargs": "^17.0.33",
    "prettier": "^3.5.3",
    "supertest": "^6.3.4",
    "testcontainers": "^11.0.3",
    "vitest": "^1.6.0",
    "zx": "^8.5.5"
